{
  "[javascript, typescript]": {
        "editor.defaultFormatter":"dbaeumer.vscode-eslint",
    },
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.eslint": true,
        "source.organizeImports": false
    },
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
}